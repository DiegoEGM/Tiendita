<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tiendita.Views.RegistroPage"
             Title="RegistroPage">
    <VerticalStackLayout>
        <Entry x:Name="txtNombre" Placeholder="Nombre"></Entry>
        <Entry x:Name="txtContra" Placeholder="Contra"></Entry>
        <Button x:Name="btnRegistrarse" Text="Registrarse" Clicked="btnRegistrarse_Clicked"></Button>
    </VerticalStackLayout>
</ContentPage>